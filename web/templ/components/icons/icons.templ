package icons

templ svg(config IconConfig) {
	<svg
		stroke={ config.Stroke }
		fill={ config.Fill }
		stroke-width={ config.StrokeWidth }
		viewBox={ config.ViewBox }
		height={ config.Height }
		width={ config.Width }
		xmlns={ config.Xmlns }
		class={ config.Class }
	>
		{ children... }
	</svg>
}

