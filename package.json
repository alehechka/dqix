{
  "name": "dqix",
  "version": "0.1.0",
  "description": "Dragon Quest IX Wiki",
  "main": "index.js",
  "scripts": {
    "build:css:dev": "tailwindcss -i web/static/input.css -o web/static/output.dev.css",
    "build:css": "NODE_ENV=production tailwindcss -i web/static/input.css -o web/static/output.css --minify",
    "watch:css": "NODE_ENV=production tailwindcss -i web/static/input.css -o web/static/output.css --minify --watch",
    "pretty": "prettier . --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alehechka/dqix.git"
  },
  "keywords": [
    "dqix",
    "templ",
    "go",
    "tailwindcss"
  ],
  "author": "alehechka",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alehechka/dqix/issues"
  },
  "homepage": "https://github.com/alehechka/dqix#readme",
  "devDependencies": {
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "tailwindcss": "^3.4.0"
  },
  "engines": {
    "npm": ">=10.1.0",
    "node": ">=v20.7.0"
  }
}
